{
  "name": "phaser-jump",
  "version": "1.0.0",
  "description": "A simple starter template for building games in Phaser with node.js tooling.",
  "main": "app.js",
  "scripts": {
    "packdev": "webpack --progress --colors --watch --config=config/webpack.dev.config.js",
    "devserver": "browser-sync start --config config/bs-config.js",
    "dev": "npm-run-all --parallel devserver packdev",
    "bsync": "browser-sync init",
    "build": "webpack --progress --colors -p --config=config/webpack.dev.config.js"
  },
  "author": "Jacob Davison",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "browser-sync": "^2.18.2",
    "css-loader": "^0.26.0",
    "eslint": "^3.11.0",
    "eslint-loader": "^1.6.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.13.0",
    "npm-run-all": "^3.1.1",
    "phaser": "^2.6.2",
    "sass-loader": "^4.0.2",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-notifier": "^1.4.1",
    "yarn": "^0.17.9"
  },
  "devDependencies": {
    "eslint": "^3.11.0",
    "eslint-config-google": "^0.7.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1"
  }
}
