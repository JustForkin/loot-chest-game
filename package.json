{
  "name": "phaser-jump",
  "version": "1.0.0",
  "description": "A simple starter template for building games in Phaser with node.js tooling.",
  "main": "app.js",
  "scripts": {
    "packdev": "webpack --progress --colors --watch --config=config/webpack.dev.config.js",
    "devserver": "browser-sync start --config config/bs-config.js",
    "dev": "npm-run-all --parallel devserver packdev",
    "bsync": "browser-sync init",
    "build": "webpack --progress --colors -p --config=config/webpack.dev.config.js"
  },
  "author": "Jacob Davison",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.18.0",
    "browser-sync": "^2.18.2",
    "css-loader": "^0.28.4",
    "electron": "^1.4.10",
    "eslint": "^4.0.0",
    "eslint-loader": "^1.6.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "globals-loader": "^0.0.4",
    "include-media": "^1.4.9",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "phaser": "^2.6.2",
    "sass-loader": "^6.0.5",
    "script-loader": "^0.7.0",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-notifier": "^1.4.1",
    "yarn": "^0.24.5"
  },
  "devDependencies": {
    "eslint": "^4.0.0",
    "eslint-config-google": "^0.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1"
  }
}
